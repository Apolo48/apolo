										<listcell>
											Entrenador 1
										</listcell>
									</listitem>
									<listitem>

										<listcell>
											Nombre Equipo 2
										</listcell>
										<listcell>Delegado 2</listcell>
										<listcell>
											Entrenador 2
										</listcell>
									</listitem>
									<listitem>

										<listcell>
