<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
   <bean id="daoTipoActividadSocial" class="dao.atleta.DaoTipoActividadSocial">
   </bean>
   <bean id="daoCurso" class="dao.atleta.DaoCurso">
   </bean>
   <bean id="servicioTipoActividadSocial" class="servicio.atleta.ServicioTipoActividadSocial">
   <property name="daoTipoActividadSocial" ref="daoTipoActividadSocial"></property>	   
   </bean>
   <bean id="servicioCurso" class="servicio.atleta.ServicioCurso">
   <property name="daoCurso" ref="daoCurso"></property>	   
   </bean>
	
</beans>